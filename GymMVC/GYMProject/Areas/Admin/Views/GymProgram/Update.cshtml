@model GymProgram

<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Update form</h4>
            <p class="card-description">
                Update form layout
            </p>
            <form asp-area="Admin" asp-controller="GymProgram" asp-action="Update" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label for="exampleInputUsername1">Category</label>
                    <input asp-for="Name" class="form-control" id="exampleInputUsername1" placeholder="Username">
                </div>
                <div class="form-group">
                    <label for="exampleInputUsername1">Image</label>
                </div>

             


                <div class="form-check form-check-flat form-check-primary">
                   
                </div>
                <button type="submit" class="btn btn-primary mr-2">Create</button>
            </form>
        </div>
    </div>
